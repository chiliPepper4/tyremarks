{
  "name": "tyremarks",
  "productName": "Tyremarks",
  "description": "Mark your biking, running and swimming activities onto maps",
  "version": "0.0.1",
  "private": false,
  "author": "Project Tyremarks",
  "copyright": "Â© 2020, Project Tyremarks",
  "license": "MIT",
  "homepage": "http://saturo.tech/tyremarks",
  "main": "app/background.js",
  "build": {
    "appId": "tech.saturo.tyremarks",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "ejs": "^3.1.3",
    "fit-file-parser": "^1.6.18",
    "fs": "0.0.1-security",
    "fs-jetpack": "^2.1.0",
    "leaflet": "^1.6.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "ejs": "^3.1.3",
    "electron": "^13.6.6",
    "electron-builder": "^22.6.0",
    "electron-mocha": "^8.1.1",
    "file-loader": "^6.0.0",
    "fit-file-parser": "^1.6.18",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "leaflet": "^1.6.0",
    "mocha": "^7.1.1",
    "path": "^0.12.7",
    "source-map-support": "^0.5.6",
    "spectron": "^10.0.1",
    "style-loader": "^1.2.1",
    "transform-loader": "^0.2.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}